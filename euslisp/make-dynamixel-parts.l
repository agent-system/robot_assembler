(defun make-triangle (a tt)
  (make-prism (reverse (list
               (float-vector (- a) 0 (- tt))
               (float-vector a 0 (- tt))
               (float-vector 0 (* (sqrt 3) a) (- tt))
               ))
              (* 2 tt)))
